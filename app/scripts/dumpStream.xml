<application>
<name>Stream Dumper</name>

      <dependencies>
         <port>/root</port>
      </dependencies>
    
      <module>
              <name>dataDumper stream</name>
              <node>localhost</node>
              <parameters>--name dump_stream </parameters>
              <tag>dataDumper stream</tag>
      </module>

      <module>
              <name>dataDumper simulation time</name>
              <node>localhost</node>
              <parameters>--name simulation_time </parameters>
              <tag>dataDumper simulation time</tag>
      </module>

    <connection>
        <from>/sot_VelKinCon_DEBUG/stream:o</from>
        <to>/dump_stream</to>
        <protocol>udp</protocol>
    </connection>

    <connection>
        <from>/clock</from>
        <to>/simulation_time</to>
        <protocol>udp</protocol>
    </connection>

</application>
