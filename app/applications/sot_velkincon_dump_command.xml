<application>
    <name>sot_VelKinCon Command Dumper</name>

      <authors>
          <author email="alessio.rocchi@iit.it">Alessio Rocchi</author>
      </authors>

      <dependencies>
         <port>/root</port>
      </dependencies>
    
      <!-- -------------------- COMMANDS ------------------------ -->

      <module>
              <name>dataDumper</name>
              <node>localhost</node>
              <parameters>--name dump_left_leg/command</parameters>
              <tag>dump_left_leg_command</tag>
      </module>

      <module>
              <name>dataDumper</name>
              <node>localhost</node>
              <parameters>--name dump_right_leg/command</parameters>
              <tag>dump_right_leg_command</tag>
      </module>
   
       <module>
              <name>dataDumper</name>
              <node>localhost</node>
              <parameters>--name dump_right_arm/command</parameters>
              <tag>dump_right_arm_command</tag>
      </module>

      <module>
              <name>dataDumper</name>
              <node>localhost</node>
              <parameters>--name dump_left_arm/command</parameters>
              <tag>dump_left_arm_command</tag>
      </module>

      <module>
              <name>dataDumper</name>
              <node>localhost</node>
              <parameters>--name dump_torso/command</parameters>
              <tag>dump_torso_command</tag>
      </module>
      
            

    <connection>
        <from>/sot_VelKinCon/left_leg/command:o</from>
        <to>/dump_left_leg/command</to>
        <protocol>udp</protocol>
    </connection>
    
    <connection>
        <from>/sot_VelKinCon/right_leg/command:o</from>
        <to>/dump_right_leg/command</to>
        <protocol>udp</protocol>
    </connection>
    
    <connection>
        <from>/sot_VelKinCon/left_arm/command:o</from>
        <to>/dump_left_arm/command</to>
        <protocol>udp</protocol>
    </connection>
    
    <connection>
        <from>/sot_VelKinCon/right_arm/command:o</from>
        <to>/dump_right_arm/command</to>
        <protocol>udp</protocol>
    </connection>
    
    <connection>
        <from>/sot_VelKinCon/torso/command:o</from>
        <to>/dump_torso/command</to>
        <protocol>udp</protocol>
    </connection>
       
</application>
